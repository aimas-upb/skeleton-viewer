<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HorizontalPanel>
		<g:PushButton ui:field="snapButton" title="Snapshot and Forward"/>
		<g:PushButton ui:field="fwdButton" title="Forward"/>
		<g:PushButton ui:field="clearButton" title="Clear Selection"/>
		<g:PushButton ui:field="downloadButton" title="Download Selection as Master file"/>
		<g:Label>Selected:&nbsp;</g:Label>
		<g:Label ui:field="selected" />
	</g:HorizontalPanel>
</ui:UiBinder> 